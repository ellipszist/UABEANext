<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=netstandard">

    <SolidColorBrush x:Key="DockApplicationAccentBrushLow">#007ACC</SolidColorBrush>
    <SolidColorBrush x:Key="DockApplicationAccentBrushMed">#0E97DD</SolidColorBrush>
    <SolidColorBrush x:Key="DockApplicationAccentBrushHigh">#1C97EA</SolidColorBrush>
    <SolidColorBrush x:Key="DockApplicationAccentForegroundBrush">#F0F0F0</SolidColorBrush>
    <SolidColorBrush x:Key="DockApplicationAccentBrushIndicator">#123456</SolidColorBrush>

    <SolidColorBrush x:Key="DockThemeBorderLowBrush" Color="#3F3F46" />
    <SolidColorBrush x:Key="DockThemeBackgroundBrush" Color="#2D2D30" />
    <SolidColorBrush x:Key="DockThemeAccentBrush" Color="#007ACC" />
    <SolidColorBrush x:Key="DockThemeForegroundBrush" Color="#F0F0F0" />
    <SolidColorBrush x:Key="DockThemeControlBackgroundBrush" Color="#123456" />
    <sys:Double x:Key="DockFontSizeNormal">12</sys:Double>
    
    <SolidColorBrush x:Key="ThemeAccentBrush" Color="#007ACC" />
    <SolidColorBrush x:Key="TabViewSelectedBackgroundBrush"  Color="#007ACC" />

    <SolidColorBrush x:Key="DockThemeListBackgroundBrush" Color="#252526" />

    <SolidColorBrush x:Key="WorkspaceItemBundleBrush" Color="#F0CA93" />
    <SolidColorBrush x:Key="WorkspaceItemAssetsBrush" Color="#B17FD7" />
    <SolidColorBrush x:Key="WorkspaceItemResourceBrush" Color="#569CD6" />
    <SolidColorBrush x:Key="WorkspaceItemOtherBrush" Color="#777777" />
    <SolidColorBrush x:Key="WorkspaceItemEtcBrush" Color="#FF4444" />

    <SolidColorBrush x:Key="TypeTextPrimitive" Color="#569CD6" />
    <SolidColorBrush x:Key="TypeTextType" Color="#4EC9B0" />
    <SolidColorBrush x:Key="TypeTextString" Color="#D69D85" />
    <SolidColorBrush x:Key="TypeTextValue" Color="#B5CEA8" />
    <SolidColorBrush x:Key="TypeTextPlain" Color="#DEDEDE" />
    <SolidColorBrush x:Key="TypeTextTheme" Color="#000000" />

</ResourceDictionary>

